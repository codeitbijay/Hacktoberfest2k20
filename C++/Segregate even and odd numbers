#include <bits/stdc++.h>
using namespace std;
int main() {
	int n;
	cin >> n;
	int *a = new int[n]; //Dynamic array allocation
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	int i=-1,j=0;
	int t; 
	while(j!=n)
	{
		if(a[j]%2==0)
		{
			i++;
			swap(a[i],a[j]); //swap is an inbuilt function
		}
		j++;
	} 
	for(int i=0;i<n;i++) 
		cout<<a[i]<<" ";
	return 0; 
}
